datasource db {
  provider = "postgresql"
  url      = "postgres://default:R5vYqt7JkQhO@ep-nameless-river-a2v123uy.eu-central-1.aws.neon.tech:5432/verceldb"
}

generator client {
  provider = "prisma-client-js"
}

model List {
  id        Int       @id @default(autoincrement())
  items     Json
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  history   History[]
  done      Boolean   @default(false)
}

model History {
  id        Int       @id @default(autoincrement())
  action    String
  listId    Int
  timestamp DateTime  @default(now())
  list      List      @relation(fields: [listId], references: [id])
}
